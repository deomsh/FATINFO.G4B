echo -e Example  FATINFO.G4B --start=1929 (cd)
echo Example  FATINFO.G4B (hd0,0)/Long\ Image\ Name\ \=\ Floppy.IMG
echo -e Example  FATINFO.G4B --skip=0x7E00 "(hd0,0)/Long Image Name = Harddisk.IMG" /T
echo
if %pagersts%==0 && pager off
endlocal
